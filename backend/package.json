  "scripts": {
    "prebuild": "rimraf dist",
    "build": "rimraf dist & nest build & npx sequelize-cli db:migrate & npx sequelize-cli db:seed:all",
    "start": "nest start",
    "start:dev": "npm run prebuild & nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "test": "jest --passWithNoTests --no-cache",
    "test:cov": "npm test -- --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:unit": "npm test -- --watch --runInBand -c jest-unit-config.js",
    "test:e2e": "npm test -- --watchAll --runInBand --config ./tests/jest-e2e.json",
    "test:e2e-silent": "npm run test:e2e -- --silent",
    "clear:jest": "jest --clearCache"
  },
  "dependencies": {
    "@nestjs/axios": "^1.0.0",
    "@nestjs/common": "^9.0.11",
    "@nestjs/core": "^9.0.11",
    "@nestjs/cqrs": "^9.0.1",
    "@nestjs/platform-express": "^9.0.11",
    "@nestjs/schedule": "^2.1.0",
    "@nestjs/sequelize": "^9.0.0",
    "@nestjs/swagger": "^6.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "pg": "^8.8.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.5",
    "sequelize": "^6.27.0",
    "sequelize-cli": "^6.5.2",
    "sequelize-typescript": "^2.1.5",
    "swagger-ui-express": "^4.4.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.1.1",
    "@nestjs/schematics": "^9.0.2",
    "@nestjs/testing": "^9.0.11",
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.1",
    "@types/node": "^18.7.14",
    "@types/sequelize": "^4.28.14",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "dotenv": "^16.0.1",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^28.1.1",
    "prettier": "^2.3.2",
    "supertest": "^6.1.3",
    "ts-jest": "^28.0.4",
    "ts-node": "^10.0.0",
